#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    int k=n;
    for(int i=1;i<=n;i++){
        for(int r=1;r<=k;r++){
            for(int c=1;c<=2*i-1;c++){
                printf("%d",i);
            }
             printf("\n");
        }
        k--;
       
    }
    return 0;
}


